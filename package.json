{
  "name": "rwkv-code-completion",
  "displayName": "rwkv-code-completion",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.106.1"
  },
  "categories": [
    "Programming Languages",
    "Machine Learning"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "rwkv-sidebar",
          "title": "RWKV 代码补全",
          "icon": "$(robot)"
        }
      ]
    },
    "views": {
      "rwkv-sidebar": [
        {
          "id": "rwkv-control-panel",
          "name": "控制面板",
          "type": "webview"
        },
        {
          "id": "rwkv-chat-panel",
          "name": "AI 聊天",
          "type": "webview"
        },
        {
          "id": "rwkv-git-commit",
          "name": "Git 提交助手",
          "type": "webview"
        }
      ]
    },
    "commands": [
      {
        "command": "rwkv-code-completion.toggleCompletion",
        "title": "RWKV: 启用/禁用代码补全",
        "icon": "$(debug-start)"
      },
      {
        "command": "rwkv-code-completion.openSettings",
        "title": "RWKV: 打开设置",
        "icon": "$(settings-gear)"
      },
      {
        "command": "rwkv-code-completion.refreshPanel",
        "title": "刷新面板",
        "icon": "$(refresh)"
      },
      {
        "command": "rwkv-code-completion.editEndpoint",
        "title": "RWKV: 编辑服务端点",
        "icon": "$(edit)"
      },
      {
        "command": "rwkv-code-completion.editMaxTokens",
        "title": "RWKV: 编辑最大Token",
        "icon": "$(edit)"
      },
      {
        "command": "rwkv-code-completion.editTemperature",
        "title": "RWKV: 编辑温度参数",
        "icon": "$(edit)"
      },
      {
        "command": "rwkv-code-completion.editTopP",
        "title": "RWKV: 编辑Top P",
        "icon": "$(edit)"
      },
      {
        "command": "rwkv-code-completion.editDebounceDelay",
        "title": "RWKV: 编辑防抖延迟",
        "icon": "$(edit)"
      }
    ],
    "menus": {
      "view/title": [
        {
          "command": "rwkv-code-completion.refreshPanel",
          "when": "view == rwkv-control-panel",
          "group": "navigation"
        }
      ],
      "view/item/context": [
        {
          "command": "rwkv-code-completion.toggleCompletion",
          "when": "view == rwkv-control-panel && viewItem == toggleButton"
        },
        {
          "command": "rwkv-code-completion.openSettings",
          "when": "view == rwkv-control-panel && viewItem == settingsButton"
        },
        {
          "command": "rwkv-code-completion.editEndpoint",
          "when": "view == rwkv-control-panel && viewItem == endpoint",
          "group": "inline"
        },
        {
          "command": "rwkv-code-completion.editMaxTokens",
          "when": "view == rwkv-control-panel && viewItem == maxTokens",
          "group": "inline"
        },
        {
          "command": "rwkv-code-completion.editTemperature",
          "when": "view == rwkv-control-panel && viewItem == temperature",
          "group": "inline"
        },
        {
          "command": "rwkv-code-completion.editTopP",
          "when": "view == rwkv-control-panel && viewItem == topP",
          "group": "inline"
        },
        {
          "command": "rwkv-code-completion.editDebounceDelay",
          "when": "view == rwkv-control-panel && viewItem == debounceDelay",
          "group": "inline"
        }
      ]
    },
    "configuration": {
      "title": "RWKV Code Completion",
      "properties": {
        "rwkv-code-completion.enabled": {
          "type": "boolean",
          "default": true,
          "description": "启用或禁用代码补全"
        },
        "rwkv-code-completion.endpoint": {
          "type": "string",
          "default": "http://192.168.0.12:8000/v3/chat/completions",
          "description": "RWKV 服务端点地址"
        },
        "rwkv-code-completion.password": {
          "type": "string",
          "default": "rwkv7_7.2b_webgen",
          "description": "RWKV 服务密码"
        },
        "rwkv-code-completion.maxTokens": {
          "type": "number",
          "default": 16,
          "description": "最大生成 token 数（8-32 推荐，越小越精准）"
        },
        "rwkv-code-completion.temperature": {
          "type": "number",
          "default": 0.1,
          "minimum": 0,
          "maximum": 2,
          "description": "生成温度（0.05-0.2 推荐，越低越确定）"
        },
        "rwkv-code-completion.topP": {
          "type": "number",
          "default": 0.95,
          "minimum": 0,
          "maximum": 1,
          "description": "Top P 采样参数（0.9-0.98 推荐）"
        },
        "rwkv-code-completion.alphaPresence": {
          "type": "number",
          "default": 0.5,
          "description": "Alpha Presence 参数"
        },
        "rwkv-code-completion.alphaFrequency": {
          "type": "number",
          "default": 0.5,
          "description": "Alpha Frequency 参数"
        },
        "rwkv-code-completion.debounceDelay": {
          "type": "number",
          "default": 300,
          "minimum": 0,
          "maximum": 5000,
          "description": "触发补全的延迟时间（毫秒）"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "pnpm run package",
    "compile": "pnpm run check-types && pnpm run lint && pnpm run build:webview && node esbuild.js",
    "watch": "npm-run-all -p watch:*",
    "watch:esbuild": "node esbuild.js --watch",
    "watch:tsc": "tsc --noEmit --watch --project tsconfig.json",
    "watch:webview": "rsbuild build --watch",
    "build:webview": "rsbuild build",
    "package": "pnpm run check-types && pnpm run lint && pnpm run build:webview && node esbuild.js --production",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "pnpm run compile-tests && pnpm run compile && pnpm run lint",
    "check-types": "tsc --noEmit",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@rsbuild/core": "^1.6.14",
    "@rsbuild/plugin-react": "^1.4.2",
    "@tailwindcss/postcss": "^4.1.17",
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@types/vscode": "^1.106.1",
    "@vscode/test-cli": "^0.0.12",
    "@vscode/test-electron": "^2.5.2",
    "autoprefixer": "^10.4.22",
    "esbuild": "^0.27.1",
    "eslint": "^9.39.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.17",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.48.1"
  },
  "dependencies": {
    "@radix-ui/react-avatar": "^1.1.11",
    "@radix-ui/react-slot": "^1.2.4",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.559.0",
    "react": "^19.2.1",
    "react-dom": "^19.2.1",
    "react-markdown": "^10.1.0",
    "rehype-highlight": "^7.0.2",
    "remark-gfm": "^4.0.1",
    "tailwind-merge": "^3.4.0"
  }
}
